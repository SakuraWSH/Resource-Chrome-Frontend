{
    "manifest_version": 3,
    "name": "Resoure-Chrome-Frontend",
    "version": "1.0.0",
    "description": "A basic Chrome extension template",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": ["icons/*.png"],
            "matches": []
        }
    ]
}
    